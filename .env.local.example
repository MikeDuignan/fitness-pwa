# GLM API Key (from https://open.bigmodel.cn/)
ZHIPU_API_KEY=your_api_key_here

# GLM Model (optional - defaults to glm-3-turbo)
# Options: glm-3-turbo, chatglm_turbo, chatglm_pro, chatglm_std
# glm-3-turbo is most compatible (recommended)
ZHIPU_MODEL=glm-3-turbo

# Database
DATABASE_URL=file:./src/db/fitness.db
